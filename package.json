{
  "name": "dollweb",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --public-url /js --out-file bundle.js",
    "build:js": "parcel watch ./public/js/index.js --out-dir ./public/js --public-url /js --out-file bundle.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.concat.css public/css/icon-font.css public/css/style.comp.css",
    "prefix:css": "postcss public/css/style.concat.css --use autoprefixer -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Thao Phuong",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bootstrap-icons": "^1.3.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0",
    "node": "^15.14.0",
    "nodemailer": "^6.6.0",
    "pug": "^3.0.0",
    "slugify": "^1.5.0",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "15.x",
    "npm": "7.x"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "postcss-cli": "^8.3.1"
  }
}
