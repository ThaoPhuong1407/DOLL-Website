{
  "name": "dollweb",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.concat.css public/css/icon-font.css public/css/style.comp.css",
    "prefix:css": "postcss public/css/style.concat.css --use autoprefixer -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Thao Phuong",
  "license": "ISC",
  "dependencies": {
    "bootstrap-icons": "^1.3.0",
    "express": "^4.17.1",
    "pug": "^3.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  }
}
