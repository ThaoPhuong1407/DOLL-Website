   }\n    } catch (err) {\n        alert(err.response.data.message);\n        console.log(err.response);\n    } \n}\n\nexport const updateCurrentData = async (type, data, loadPage, id) => {\n    try {\n        const res = await axios({\n            method: 'PATCH',\n            url: `http://localhost:3000/api/${type}/${id}`, \n            data: data,    \n        });\n\n        if (res.data.status === 'success') {\n            alert(`Update ${type.toUpperCase()} Successfully`);\n            window.setTimeout(() => {\n                location.assign(`/${loadPage}`);\n            }, 1000);\n        }\n    } catch (err) {\n        alert(err.response.data.message);\n        console.log(err.response);\n    } \n}\n\nexport const deleteCurrentData = async (type, id) => {\n    try {\n        const res = await axios({\n            method: 'DELETE',\n            url: `http://localhost:3000/api/${type}/${id}`,    \n        });\n\n        if (res.status === 204) {\n            alert(`Delete ${type.toUpperCase()} Successfully`);\n            // location.reload(true);\n            window.setTimeout(() => {\n                location.assign(`/`);\n            }, 1000);\n        }\n    } catch (err) {\n        alert(err.response.data.message);\n        console.log(err.response);\n    } \n}"},"lineCount":null}},"error":null,"hash":"8e610b6882feff65edbf4747de682b37","cacheData":{"env":{}}}